                android:text="sffghxscfgh"
                android:id="@+id/te2"
                android:layout_marginTop="-20dp"
                android:layout_marginLeft="105dp"
                android:textColor="#000000"
                />

            <com.dsource.idc.jellow.CircularImageView
                android:id="@+id/icon3"
