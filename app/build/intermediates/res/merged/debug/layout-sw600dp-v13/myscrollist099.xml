                android:layout_toRightOf="@+id/icon2"
                />

            <TextView
                android:layout_width="320dp"
                android:layout_height="wrap_content"
                android:textSize="16.2dp"
                android:textAllCaps="true"
                android:textAlignment="center"
                android:layout_below="@+id/icon3"
